{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "ts": "tsc --noEmit --skipLibCheck",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "commit": "commit",
    "test:app": "jest --testPathPattern=__tests__/App.test.tsx",
    "test:screens": "jest --testPathPattern=src/screens/__tests__",
    "test:components": "jest --testPathPattern=src/components/__tests__",
    "test:containers": "jest --testPathPattern=src/containers/__tests__",
    "adb": "adb devices && adb reverse tcp:8081 tcp:8081",
    "load-splash-asset": "npx react-native generate-bootsplash ./assets/logo.png --background=FFFFFF --logo-width=200 --assets-output=./assets",
    "android:dev": "cd android && gradlew clean && cd .. && npx react-native run-android --active-arch-only --mode developmentdebug",
    "android:prod": "cd android && gradlew clean && cd .. && npx react-native run-android --active-arch-only --mode productiondebug",
    "android:qa": "cd android && gradlew clean && cd .. && npx react-native run-android --active-arch-only --mode qadebug",
    "android:stage": "cd android && gradlew clean && cd .. && npx react-native run-android --active-arch-only --mode stagedebug"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "6.4.0",
    "@react-navigation/native": "^6.1.12",
    "@react-navigation/native-stack": "^6.9.20",
    "@react-navigation/stack": "^6.3.23",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-bootsplash": "^5.4.1",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-paper": "^4.12.5",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.29.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.0.0",
    "conventional-changelog-atom": "^4.0.0",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.60",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.6.3",
    "msw": "^2.2.2",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
