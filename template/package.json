{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "ts": "tsc --noEmit --skipLibCheck",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "commit": "commit",
    "test:app": "jest --testPathPattern=__tests__/App.test.tsx",
    "test:screens": "jest --testPathPattern=src/screens/__tests__",
    "test:components": "jest --testPathPattern=src/components/__tests__",
    "test:containers": "jest --testPathPattern=src/containers/__tests__",
    "adb": "adb devices && adb reverse tcp:8081 tcp:8081",
    "android-tasks": "cd android && gradlew tasks",
    "load-splash-asset": "npx react-native generate-bootsplash ./assets/logo.png --background=FFFFFF --logo-width=192 --assets-output=./assets",
    "android:dev": "cd android && gradlew clean && gradlew installDevelopmentDebug",
    "android:stage": "cd android && gradlew clean && gradlew installStageDebug",
    "android:prod": "cd android && gradlew clean && gradlew installProductionDebug",
    "android:qa": "cd android && gradlew clean && gradlew installQaDebug"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "6.4.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^1.9.7",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-native": "0.76.4",
    "react-native-bootsplash": "^5.5.3",
    "react-native-config": "^1.5.3",
    "react-native-error-boundary": "^1.2.5",
    "react-native-gesture-handler": "^2.19.0",
    "react-native-keyboard-controller": "^1.14.4",
    "react-native-mmkv-storage": "^0.11.2",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.1",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.13",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native/babel-preset": "0.76.4",
    "@react-native/eslint-config": "0.76.4",
    "@react-native/metro-config": "0.76.4",
    "@react-native/typescript-config": "0.76.4",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "conventional-changelog-atom": "^4.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.6.3",
    "msw": "^2.6.4",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "rnx-gen": "^1.1.4",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
